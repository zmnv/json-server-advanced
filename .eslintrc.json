{
    "globals": {
      "chrome": true,
      "location": true,
      "property": true
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 9
    },
    "extends": "eslint:recommended",
    "rules": {
      "comma-dangle": 0,
      "eqeqeq": 0,
      "camelcase": 0,
      "consistent-return": 0,
      "global-require": 0,
      "new-cap": 0,
      "no-console": 0,
      "spaced-comment": 0,
      "no-restricted-globals": 0,
      "no-shadow-restricted-names": 2
    }
  }
  